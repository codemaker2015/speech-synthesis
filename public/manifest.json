{
  "short_name": "React App",
  "name": "Create React App Sample",
  "manifest_version": 2,
  "version": "1.0",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": ["fullscreen"],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  }
}
